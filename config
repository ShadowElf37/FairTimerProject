await_time 5:00pm
prompt "Preshow"
countdown 20m -12m(play "10 minutes") -7m(play "5 minutes") -2m(play "places act 1") -2m(prompt "Preshow - Places")
prompt "Act 1"
countup
prompt "Intermission"
countdown 15m -7m(play "5 minutes act 2") -2m(play "places act 2")
prompt "Act 2"
countup
